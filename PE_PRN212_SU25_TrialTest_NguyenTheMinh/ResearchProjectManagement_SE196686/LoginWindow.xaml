<Window x:Class="ResearchProjectManagement_SE196686.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResearchProjectManagement_SE196686"
        mc:Ignorable="d"
        Title="Login Window SU 2025" Height="400" Width="700" FontFamily="Tahoma" FontSize="26" Background="#FFF5F5DC" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Label Content="Email"/>
        <TextBox x:Name="txtEmail"/>
        <Label Content="Password"/>
        <TextBox x:Name="txtPassword"/>
        <Button x:Name="btnLogin" Content="Login" Click="btnLogin_Click"/>
    </StackPanel>
</Window>
